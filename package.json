{
	"name": "budgetsaurus",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "npm run dev",
		"dev": "pollen && vite dev",
		"build": "pollen && vite build",
		"start:wrangler": "wrangler pages dev .svelte-kit/cloudflare --live-reload",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "eslint .",
		"wrangler": "wrangler",
		"drizzle-kit": "drizzle-kit",
		"gen-migrate": "NODE_ENV=development drizzle-kit generate",
		"dev-migrate": "NODE_ENV=development drizzle-kit migrate",
		"prod-migrate": "NODE_ENV=production drizzle-kit migrate",
		"dev-seed": "NODE_ENV=development tsx ./drizzle/dev.seed.ts",
		"tsx": "tsx"
	},
	"dependencies": {
		"@faker-js/faker": "^9.0.2",
		"@fontsource/fira-sans": "^5.1.0",
		"@fontsource/ubuntu-mono": "^5.1.0",
		"@libsql/client": "^0.14.0",
		"@types/jsonwebtoken": "^9.0.7",
		"bcryptjs": "^2.4.3",
		"chart.js": "^4.4.4",
		"date-fns": "^4.1.0",
		"drizzle-orm": "^0.33.0",
		"jsonwebtoken": "^9.0.2",
		"pollen-css": "^5.0.1",
		"postcss-preset-env": "^10.0.5",
		"redaxios": "^0.5.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^4.7.2",
		"@sveltejs/kit": "^2.5.28",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/bcryptjs": "^2.4.6",
		"@types/eslint": "^9.6.1",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.24.2",
		"eslint": "^9.11.1",
		"eslint-plugin-svelte": "^2.44.0",
		"globals": "^15.9.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.7.0",
		"vite": "^5.4.7",
		"vitest": "^2.1.1",
		"wrangler": "^3.78.8"
	},
	"browser": {
		"fs": false
	},
	"volta": {
		"node": "20.17.0",
		"npm": "10.8.2"
	}
